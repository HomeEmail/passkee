{
    "name": "passkee",
    "version": "1.1.2",
    "description": "Records your browser interactions and automatically generates Puppeteer script.",
    "scripts": {
        "build": "node build/build.js",
        "build-w": "webpack --config build/webpack-conf/build.js -w",
        "dev": "webpack-dev-server --inline --hot --config build/webpack-conf/dev.js",
        "test": "mocha test/index.js --recursive --full-trace --timeout 10000",
        "test-dev": "mocha test/index.js --recursive --inspect --full-trace --bail --debug --async-only --timeout 10000"
    },
    "main": "serve/index.js",
    "files": [
        "serve",
        "typings",
        "browser",
        "common",
        "index.js"
    ],
    "devDependencies": {
        "autoprefixer": "^6.7.2",
        "babel-core": "^6.26.3",
        "babel-eslint": "^7.1.1",
        "babel-loader": "^7.1.4",
        "babel-plugin-transform-async-to-generator": "^6.24.1",
        "babel-plugin-transform-decorators-legacy": "^1.3.5",
        "babel-plugin-transform-runtime": "^6.23.0",
        "babel-preset-es2015": "^6.24.1",
        "babel-runtime": "^6.26.0",
        "colors": "^1.3.2",
        "copy-webpack-plugin": "^4.0.1",
        "css-loader": "^0.28.11",
        "extract-text-webpack-plugin": "^4.0.0-beta.0",
        "file-loader": "^1.1.11",
        "fs-extra": "^7.0.0",
        "glob": "^7.1.2",
        "html-loader": "^0.5.5",
        "html-webpack-plugin": "^3.0.6",
        "style-loader": "^0.20.3",
        "stylus": "^0.54.5",
        "stylus-loader": "^3.0.2",
        "url-loader": "^1.0.1",
        "vue-html-loader": "^1.2.4",
        "vue-loader": "^14.2.3",
        "vue-style-loader": "^3.0.1",
        "vue-template-compiler": "^2.5.15",
        "webpack": "^4.20.2",
        "webpack-cli": "^3.1.1",
        "webpack-dev-server": "^3.1.4",
        "webpack-merge": "^4.1.0",
        "yargs": "^11.0.0",
        "mini-css-extract-plugin": "^0.6.0",
        "mocha": "^6.1.3",
        "@medv/finder": "^1.1.1",
        "codemirror": "^5.45.0",
        "flex.css": "^1.1.7",
        "vue": "^2.5.15",
        "vue-codemirror": "^4.0.6"
